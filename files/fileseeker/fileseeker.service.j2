[Unit]
Description=Unibo Fileseeker
Documentation=https://github.com/cartabinaria/fileseeker

After=network.target

[Service]
User=fileseeker
Group=fileseeker
ReadWriteDirectories=/opt/fileseeker/
WorkingDirectory=/opt/fileseeker/source/
ExecStart=/opt/fileseeker/fileseeker -c /opt/fileseeker/config.toml
